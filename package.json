{
  "name": "nahida-desktop",
  "version": "0.0.3",
  "description": "Nahida.live Desktop App",
  "main": "./out/main/index.js",
  "author": "nahida.live",
  "homepage": "https://nahida.live",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:mac": "npm run build && electron-builder --mac",
    "build:linux": "npm run build && electron-builder --linux",
    "release": "npm run build && electron-builder --win"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@elysiajs/node": "^1.2.6",
    "@formkit/auto-animate": "^0.8.2",
    "@tanstack/svelte-query": "^5.75.7",
    "@types/better-sqlite3": "^7.6.13",
    "@types/electron-progressbar": "^1.2.7",
    "@types/node-fetch": "^2.6.12",
    "animejs": "^4.0.2",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.10.0",
    "bindings": "^1.5.0",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.0",
    "electron-overlay-window": "github:myparsleycat/electron-overlay-window",
    "electron-updater": "^6.6.2",
    "elysia": "^1.3.1",
    "file-type": "^20.5.0",
    "lucide-svelte": "^0.509.0",
    "mode-watcher": "^1.0.7",
    "nan": "^2.22.2",
    "nanoid": "^5.1.5",
    "node-gyp": "^11.2.0",
    "paneforge": "^0.0.6",
    "svelte-i18n": "^4.0.1",
    "svelte-sonner": "^0.3.28",
    "watcher": "^2.3.1"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@internationalized/date": "^3.8.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@types/node": "^22.15.17",
    "autoprefixer": "^10.4.21",
    "bits-ui": "0.22.0",
    "clsx": "^2.1.1",
    "electron": "^35.2.1",
    "electron-builder": "^26.0.12",
    "electron-progressbar": "^2.2.1",
    "electron-vite": "^3.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "tailwind-merge": "^3.2.0",
    "tailwind-variants": "^1.0.0",
    "tailwindcss": "3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  }
}